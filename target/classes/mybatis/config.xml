<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
  
<configuration>
	<!-- DB컬럼명을 Java의 Camel표기법으로 변경 -->
	<settings>
		<setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>
	<typeAliases>
		<!--타입얼리어스 -->
		<typeAlias type="com.covid19.app.admin.model.vo.AdminMember" alias="ADMIN"/>
		<typeAlias type="com.covid19.app.admin.model.vo.AdminTestVo" alias="TEST"/>
	</typeAliases>
	<mappers>
		<!-- 매퍼매핑 -->
		<mapper resource="/mybatis/mapper/AdminMapper.xml"/>
	</mappers>
	
</configuration>